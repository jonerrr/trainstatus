  "query": "\n\t\tSELECT\n\t\tt.id,\n\t\tt.route_id,\n\t\tt.direction,\n\t\tt.vehicle_id,\n\t\tt.created_at,\n\t\tt.deviation,\n\t\tbp.lat,\n\t\tbp.lon,\n\t\tbp.progress_status,\n\t\tbp.passengers,\n\t\tbp.capacity,\n\t\tbp.stop_id,\n\t\t(\n\t\tSELECT\n\t\t\tbrs.headsign\n\t\tFROM\n\t\t\tbus_route_stops brs\n\t\tWHERE\n\t\t\tbrs.route_id = t.route_id\n\t\t\tAND brs.direction = t.direction\n\t\tLIMIT 1) AS headsign\n\tFROM\n\t\tbus_trips t\n\tLEFT JOIN bus_positions bp ON\n\t\tbp.vehicle_id = t.vehicle_id\n\t\tAND bp.mta_id = t.mta_id\n\t\tAND t.id = ANY(\n\t\tSELECT\n\t\t\tt.id\n\t\tFROM\n\t\t\tbus_trips t\n\t\tLEFT JOIN bus_stop_times st ON\n\t\t\tst.trip_id = t.id\n\t\tWHERE\n\t\t\tst.arrival BETWEEN now() AND (now() + INTERVAL '4 hours'))\n\tWHERE\n\t\tbp.lat IS NOT NULL\n\t\tAND bp.lon IS NOT NULL",
