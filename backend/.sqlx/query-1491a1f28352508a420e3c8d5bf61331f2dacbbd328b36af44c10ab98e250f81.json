  "query": "SELECT\n            bst.*, bt.route_id\n        FROM\n            bus_stop_times bst\n        LEFT JOIN bus_trips bt ON\n            bt.id = bst.trip_id\n        WHERE\n            bt.route_id = ANY($1)\n            AND bst.arrival BETWEEN $2 AND ($2 + INTERVAL '4 hours')\n        ORDER BY\n            bst.arrival",
