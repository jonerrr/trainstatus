  "query": "\nSELECT\n\tt.id,\n\tt.route_id,\n\tt.direction,\n\tt.vehicle_id,\n\tt.created_at,\n\tt.deviation,\n\tbp.lat,\n\tbp.lon,\n\tbp.progress_status,\n\tbp.passengers,\n\tbp.capacity,\n\tbp.stop_id,\n\t(\n\tSELECT\n\t\tbrs.headsign\n\tFROM\n\t\tbus_route_stops brs\n\tWHERE\n\t\tbrs.route_id = t.route_id\n\t\tAND brs.direction = t.direction\n\tLIMIT 1) AS headsign\nFROM\n\tbus_trips t\nLEFT JOIN bus_positions bp ON\n\tbp.vehicle_id = t.vehicle_id\n\tAND bp.mta_id = t.mta_id\n\tAND t.id = ANY(\n\tSELECT\n\t\tt.id\n\tFROM\n\t\tbus_trips t\n\tLEFT JOIN bus_stop_times st ON\n\t\tst.trip_id = t.id\n\tWHERE\n\t\tst.arrival BETWEEN $1 AND ($1 + INTERVAL '4 hours'))",
