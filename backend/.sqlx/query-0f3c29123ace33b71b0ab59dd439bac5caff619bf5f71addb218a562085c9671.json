{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                s.*,\n                ARRAY_AGG(JSONB_BUILD_OBJECT('id',\n                rs.route_id,\n                'stop_type',\n                rs.stop_type)) AS routes\n            FROM\n            stops s\n                LEFT JOIN route_stops rs ON\n            s.id = rs.stop_id\n            WHERE\n                s.id = ANY($1)\n            GROUP BY\n                s.id\n            ",
