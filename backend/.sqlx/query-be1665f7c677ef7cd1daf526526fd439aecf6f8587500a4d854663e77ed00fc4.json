{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            bst.*\n        FROM\n            bus_stop_times bst\n        LEFT JOIN bus_trips bt ON\n            bt.id = bst.trip_id\n        WHERE\n            bt.route_id = ANY($1)\n            AND bst.arrival > now()\n        ORDER BY\n            bst.arrival",
